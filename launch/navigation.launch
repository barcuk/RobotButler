<launch>
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
		<rosparam file="$(find robot_butler)/costmap_common_params.yaml" command="load" ns="global_costmap" />
		<rosparam file="$(find robot_butler)/costmap_common_params.yaml" command="load" ns="local_costmap" />
		<rosparam file="$(find robot_butler)/local_costmap_params.yaml" command="load" />
		<rosparam file="$(find robot_butler)/global_costmap_params.yaml" command="load" />
		<rosparam file="$(find robot_butler)/base_local_planner_params.yaml" command="load" />
		<param name="base_global_planner" type="string" value="NavfnROS" />
		<param name="conservative_reset_dist" type="double" value="3.0" />
		<param name="controller_frequency" type="double" value="15.0" />
	</node>
</launch>
